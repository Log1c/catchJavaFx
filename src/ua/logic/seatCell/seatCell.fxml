<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>
<?import java.net.*?>
<Group fx:id="root"
       xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
       fx:controller="ua.logic.seatCell.SeatCellController">
    <stylesheets>
        <URL value="@seat.css" />
    </stylesheets>

    <HBox prefHeight="40" prefWidth="346" spacing="9">
        <VBox>
            <Label text="SEAT"/>
            <Label fx:id="seatNumber"/>
        </VBox>
        <VBox HBox.hgrow="ALWAYS">
            <HBox>
                <Label fx:id="cardHolderFirstName"/>
                <Label fx:id="cardHolderLastName"/>
            </HBox>
            <Label fx:id="cardDescription"/>
        </VBox>
        <Label fx:id="amountLabel" maxHeight="Infinity"/>
    </HBox>
</Group>